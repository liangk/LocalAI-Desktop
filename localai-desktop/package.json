{
  "name": "localai-desktop",
  "main": "electron.main.ts",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 7311",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "electron:start": "ng build && electron .",
    "electron:package": "ng build && electron-forge package",
    "electron:make": "ng build && electron-forge make",
    "lint": "ng lint",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/router": "^20.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^20.0.0",
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.0",
    "@electron-forge/cli": "^7.9.0",
    "@types/jasmine": "~5.1.0",
    "electron": "38.1.2",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
